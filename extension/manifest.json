{
    "manifest_version": 3,
    "name": "OCR Screen Selection",
    "version": "1.0",
    "permissions": ["scripting", "activeTab", "storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "<all_urls>"
    ]
}
